{
    "name": "melhorenvio/shipment-sdk-php",
    "description": "Este pacote é responsável por realizar o cálculo de cotações.",
    "keywords": [
        "melhorenvio",
        "shipment-sdk-php"
    ],
    "homepage": "https://github.com/melhorenvio/shipment-sdk-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Melhor Envio",
            "email": "tecnologia@melhorenvio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/polyfill-php74": "^1.23"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.6",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MelhorEnvio\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MelhorEnvio\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
